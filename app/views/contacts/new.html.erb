<h1>New Contact</h1>

<% if @contact.errors.any? %>
<p>You suck!</p>
<ul>
<% @contact.errors.full_message.each do |message| %>
  <li><%= message %></li>
  <% end %>
</ul>
<% end %>



<%= form_for @contact do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name%>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>
  <%= f.submit :submit %>

<% end %>
